{
  "name": "developer.gov.sg",
  "version": "0.0.0",
  "description": "developer.gov.sg",
  "repository": {
    "type": "git",
    "url": "https://github.com/govtechsg/developer.gov.sg.git"
  },
  "license": "ISC",
  "scripts": {
    "build:dev": "npm run test:site && webpack && bundle exec jekyll build",
    "build:site": "npm run build:static && JEKYLL_ENV=production bundle exec jekyll build",
    "build:static": "webpack --mode=production",
    "deploy": "npm run test:site && npm run build:site",
    "dev:concurrent": "concurrently \"npm run dev:static\" \"npm run dev:jekyll\"",
    "dev:jekyll": "bundle exec jekyll serve",
    "dev:netlify": "NODE_ENV=development netlify dev",
    "dev:static": "webpack --watch",
    "test:site": "NODE_ENV=production mocha --require @babel/register --recursive ./apps/src/tests/**/*.js"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.2.0",
    "css-loader": "^3.4.0",
    "date-fns": "^2.14.0",
    "diff2html": "^2.12.2",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^4.3.0",
    "html-loader": "^0.5.5",
    "lodash": "^4.17.19",
    "lunr": "^2.3.8",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.8.2",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.1",
    "noty": "^3.2.0-beta",
    "sanitize-html": "^1.20.1",
    "sass-loader": "^8.0.0",
    "sgds-govtech": "^1.3.14",
    "style-loader": "^1.1.2",
    "tiptap": "^1.26.6",
    "tiptap-extensions": "^1.28.6",
    "url-loader": "^2.3.0",
    "v-tooltip": "^2.0.2",
    "vue": "^2.6.11",
    "vue-cookies": "^1.6.1",
    "vue-loader": "^15.8.3",
    "vue-loading-overlay": "^3.2.0",
    "vue-moment": "^4.1.0",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10"
  }
}
